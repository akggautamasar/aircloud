
# Telegram API credentials
API_ID=your_api_id
API_HASH=your_api_hash

# Bot tokens (comma-separated for multiple bots)
BOT_TOKENS=bot_token_1,bot_token_2

# Premium account sessions (comma-separated)
STRING_SESSIONS=session_1,session_2

# Main bot for bot mode (optional)
MAIN_BOT_TOKEN=main_bot_token

# Storage configuration
STORAGE_CHANNEL=-1001234567890
TELEGRAM_ADMIN_IDS=123456789,987654321

# Admin access
ADMIN_PASSWORD=your_secure_password

# Supabase configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Server configuration
PORT=8000
PING_INTERVAL=300
PING_URL=https://your-app.koyeb.app

# File limits (bytes)
MAX_FILE_SIZE=4294967296
MAX_FILE_SIZE_BOT=2147483648
